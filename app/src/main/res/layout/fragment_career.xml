<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.setting.SettingFragment">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="-155dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayoutCompat"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/point_box"
                android:layout_width="match_parent"
                android:layout_height="125dp"
                android:layout_marginStart="14dp"
                android:layout_marginTop="159dp"
                android:layout_marginEnd="16dp"
                android:background="@drawable/bg_solid_light_blue_radius_16_5"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/upper_point_box"
                android:layout_width="match_parent"
                android:layout_height="125dp"
                android:layout_marginStart="14dp"
                android:layout_marginEnd="116dp"
                android:background="@drawable/bg_solid_blue_radius_16_5"
                app:layout_constraintTop_toTopOf="@+id/point_box"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/point_box"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/certificate_box"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="14dp"
                android:background="@drawable/bg_light_yellow_radius_16_5"
                app:layout_constraintTop_toBottomOf="@+id/upper_point_box"
                app:layout_constraintStart_toStartOf="@+id/point_box"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintWidth_percent="0.44"
                app:layout_constraintHeight_percent="0.23375"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/contest_box"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/bg_light_yellow_radius_16_5"
                app:layout_constraintTop_toTopOf="@+id/certificate_box"
                app:layout_constraintEnd_toEndOf="@+id/point_box"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintWidth_percent="0.44"
                app:layout_constraintHeight_percent="0.23375"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/volunteer_box"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="14dp"
                android:background="@drawable/bg_light_yellow_radius_16_5"
                app:layout_constraintTop_toBottomOf="@+id/certificate_box"
                app:layout_constraintStart_toStartOf="@+id/point_box"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintWidth_percent="0.44"
                app:layout_constraintHeight_percent="0.23375"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/activities_box"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/bg_light_yellow_radius_16_5"
                app:layout_constraintTop_toTopOf="@+id/volunteer_box"
                app:layout_constraintEnd_toEndOf="@+id/point_box"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintWidth_percent="0.44"
                app:layout_constraintHeight_percent="0.23375"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.constraintlayout.widget.ConstraintLayout>